   <script>
var timeout;

$('#cart').on({
  mouseenter: function() {
    $('#cart-dropdown').show();
  },
  mouseleave: function() {
    timeout = setTimeout(function() {
      $('#cart-dropdown').hide();
    }, 200);
  }
})

// laisse le contenu ouvert Ã  son survol
// le cache quand la souris sort
$('#cart-dropdown').on({
  mouseenter: function() {
    clearTimeout(timeout);
  },
  mouseleave: function() {
    $('#cart-dropdown').hide();
  }
});

</script>

